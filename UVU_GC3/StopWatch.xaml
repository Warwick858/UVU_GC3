    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UVU_GC3"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignWidth="328.319" Height="89.442">

    <StackPanel Orientation="Horizontal">
        <RepeatButton x:Name="seekBck" Content="&lt;&lt;" Style="{StaticResource watchBtnStyle}" HorizontalAlignment="Right" Margin="34,40,0,0" VerticalAlignment="Center"/>
        <RepeatButton x:Name="seekFwd" Content=">>" HorizontalAlignment="Right" Margin="34,40,0,0" VerticalAlignment="Center"/>
        

    </StackPanel>
</UserControl>
